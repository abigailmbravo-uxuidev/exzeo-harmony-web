{
  "name": "harmony-web",
  "version": "0.6.7",
  "private": true,
  "scripts": {
    "server": "./replace-dotenv.sh && exframe-web",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "lint": "eslint src --ext .jsx --ext .js",
    "start": "react-scripts start",
    "cypress": "react-scripts cypress",
    "docker": "rm .dockerignore; cp harmony-web.dockerignore .dockerignore; docker build .",
    "docker:cypress": "rm .dockerignore; cp cypress.dockerignore .dockerignore; docker build . -f Dockerfile.cypress"
  },
  "dependencies": {
    "@exzeo/core-ui": "^0.4.1",
    "@exzeo/react-scripts": "1.0.4",
    "auth0-js": "~8.10.1",
    "axios": "~0.15.2",
    "classnames": "~2.2.5",
    "exframe-web": "^1.0.0",
    "font-awesome": "~4.7.0",
    "history": "~4.6.1",
    "immutable": "~3.8.2",
    "jwt-decode": "~2.2.0",
    "localStorage": "~1.0.3",
    "lodash": "~4.17.4",
    "moment": "~2.19.3",
    "moment-timezone": "~0.5.14",
    "platform": "~1.3.4",
    "prop-types": "~15.6.0",
    "query-string": "~0.2.0",
    "react": "16.6.x",
    "react-app-polyfill": "^0.2.0",
    "react-bootstrap-table": "~4.3.1",
    "react-countup": "~3.0.3",
    "react-dom": "16.6.x",
    "react-helmet": "~5.2.0",
    "react-modal": "~3.1.7",
    "react-number-format": "~3.0.3",
    "react-redux": "~5.0.6",
    "react-router": "~4.2.0",
    "react-router-dom": "~4.2.2",
    "react-select": "~1.1.0",
    "react-text-mask": "~5.0.2",
    "react-tooltip": "~3.4.0",
    "redux": "~3.7.2",
    "redux-batched-actions": "~0.1.6",
    "redux-form": "~6.6.0",
    "redux-persist": "~4.10.2",
    "redux-thunk": "~2.2.0",
    "validator": "~7.2.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "~1.10.0",
    "cypress": "^3.1.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "istanbul": "~0.4.5",
    "jest-enzyme": "~4.0.1",
    "redux-mock-store": "~1.3.0",
    "sinon": "~2.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/routes.js",
      "!src/setupTests.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 70
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
