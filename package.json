{
  "name": "harmony-web",
  "version": "0.1.1",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "mockery": "^2.0.0",
    "node-sass": "^4.0.0",
    "nyc": "^10.1.2",
    "opn": "^4.0.2",
    "react-addons-test-utils": "^15.4.1",
    "react-scripts": "0.7.0",
    "shelljs": "^0.7.5",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "apollo-client": "0.5.0",
    "auth0-js": "^7.4.0",
    "axios": "^0.15.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "ldclient-js": "^1.1.4",
    "localStorage": "^1.0.3",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "react": "^15.3.2",
    "react-apollo": "^0.5.16",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^4.0.0-alpha.5",
    "react-router-dom": "^4.0.0-beta.3",
    "react-simple-currency": "^1.0.11",
    "react-tooltip": "^3.2.2",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-mock-store": "^1.2.2",
    "redux-thunk": "^2.1.0",
    "typeahead": "^0.2.2",
    "validator": "^6.2.1"
  },
  "scripts": {
    "build": "react-scripts build && node scripts/genEjs",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "local": "react-scripts start",
    "push": "npm run build && docker build -f dev.dockerfile -t exzeo/harmony-web:develop . && docker push exzeo/harmony-web:develop",
    "both": "node scripts/start-both",
    "start": "node scripts/start-api",
    "dev": "react-scripts start",
    "coverage": "cross-env NODE_ENV=test nyc npm test",
    "test": "mocha --reporter spec utils/testSetup \"src/**/*.spec.js\"",
    "test::all": "npm run lint && npm run coverage",
    "test::watch": "npm test -- --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "airbnb",
      "stage-0"
    ],
    "plugins": [
      "babel-plugin-transform-decorators-legacy"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  }
}
