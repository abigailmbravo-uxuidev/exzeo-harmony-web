{
  "name": "harmony-web",
  "version": "0.6.7",
  "private": true,
  "scripts": {
    "server": "./replace-dotenv.sh && exframe-web",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test",
    "lint": "eslint src --ext .jsx --ext .js",
    "start": "react-scripts start",
    "cypress": "react-scripts cypress"
  },
  "dependencies": {
    "@exzeo/core-ui": "0.9.0",
    "@exzeo/react-scripts": "2.3.0",
    "auth0-js": "~8.10.1",
    "classnames": "~2.2.5",
    "core-js": "^2.6.9",
    "font-awesome": "~4.7.0",
    "history": "~4.6.1",
    "jwt-decode": "~2.2.0",
    "localStorage": "~1.0.3",
    "lodash": "^4.17.15",
    "moment": "~2.19.3",
    "moment-timezone": "~0.5.14",
    "prop-types": "~15.6.0",
    "query-string": "~0.2.0",
    "react": "16.8.4",
    "react-app-polyfill": "^0.2.0",
    "react-bootstrap-table": "~4.3.1",
    "react-dom": "16.8.4",
    "react-helmet": "^5.2.1",
    "react-modal": "~3.1.7",
    "react-redux": "~5.0.6",
    "react-router-dom": "^5.0.0",
    "react-tooltip": "~3.4.0",
    "redux": "~3.7.2",
    "redux-form": "~6.6.0",
    "redux-persist": "~4.10.2",
    "redux-thunk": "~2.2.0",
    "reselect": "^4.0.0",
    "validator": "~7.2.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "~1.10.0",
    "cypress": "^3.4.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "husky": "^3.0.0",
    "istanbul": "~0.4.5",
    "jest-dom": "^3.4.0",
    "jest-enzyme": "^7.0.2",
    "jest-teamcity-reporter": "^0.9.0",
    "lint-staged": "^9.0.2",
    "prettier": "^1.18.2",
    "react-testing-library": "^6.0.3",
    "redux-mock-store": "~1.3.0",
    "sinon": "~2.4.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "./.eslintrc"
    ]
  },
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/routes.js",
      "!src/state/reducers/initialState.js",
      "!src/setupTests.js",
      "!src/**/choreographer.js",
      "!src/**/reduxFormFailSubmit.js",
      "!src/test-utils/fixtures/*",
      "!src/test-utils/index.js",
      "!src/mock-data/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 60
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss,json,md}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "cypress/**/*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
